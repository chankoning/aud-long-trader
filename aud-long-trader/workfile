        if ($('#guidelinesCheckbox'+5).attr('checked') == 'checked')
            $('#guidelinesButton'+5).removeClass('disabled');
        else
            $('#guidelinesButton'+5).addClass('disabled');
    });

    $('a[id^="guidelinesButton"]').on('click', function(){
        if ($(this).hasClass('disabled')) {
            return false;
        }
    });
