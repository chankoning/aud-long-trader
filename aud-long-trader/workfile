<script type="text/javascript">

	function onBigX() {
		var slotLeaderboard;
		$('#theBigX').fadeOut('fast');

		// click tracking issue 27402
		ga('create', 'UA-2555300-55', 'auto', {name: 'allSitesTracker'});
		ga('send', 'event', 'Homepage Billboard', 'X-Close Billboard');
		////////////////////////////////

		$('.midHeader').animate({
			height: '90px'
		},200 );
		googletag.cmd.push(function() {
			slotLeaderboard = googletag.defineSlot('/6938/FP_AU_site/FP_AU_Leaderboard_Default', [[728, 90], [970, 90]], 'div-gpt-ad-1433408666196-2').setTargeting('ad_group', Adomik.randomAdGroup()).addService(googletag.pubads());
			googletag.cmd.push(function() { googletag.display('div-gpt-ad-1433408666196-2'); });
		});
		refreshSlot(slotLeaderboard);
		$('#div-gpt-ad-1441189641022-1').hide();
		$('#div-gpt-ad-1433408666196-2').show();
	}

	function getMaxLeaderBoard() {
		return (getCookie("geoC") || '').toLowerCase() === 'us' ? 5 : 3;
	}

	function checkBigAd() {
		var counter = +getCookie("billboardCounter_70") || 0;
		if( true && counter === 0 && true) {
			$('.midHeader').height('250px');
			$('#div-gpt-ad-1441189641022-1').show();
			$('#div-gpt-ad-1433408666196-2').hide();
			$('#theBigX').show();
			googletag.cmd.push(function() {
				googletag.defineSlot('/6938/FP_AU_site/FP_AU_Billboard_Default',  [970, 250] , 'div-gpt-ad-1441189641022-1').setTargeting('ad_group', Adomik.randomAdGroup()).addService(googletag.pubads());
				googletag.cmd.push(function() { googletag.display('div-gpt-ad-1441189641022-1'); });
			});
		} else {
			googletag.cmd.push(function() {
				googletag.defineSlot('/6938/FP_AU_site/FP_AU_Leaderboard_Default', [[728, 90], [970, 90]], 'div-gpt-ad-1433408666196-2').setTargeting('ad_group', Adomik.randomAdGroup()).addService(googletag.pubads());
				googletag.cmd.push(function() { googletag.display('div-gpt-ad-1433408666196-2'); });
			});
		}

		counter++;
		if(counter === getMaxLeaderBoard()) {
			counter = 0;
		}
		setCookie("billboardCounter_70", counter);
	}

</script>
