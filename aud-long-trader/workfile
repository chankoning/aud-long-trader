<script type="text/javascript">

    (function($){
        var now = new Date(1489713154000), current;
        var maxDate = now.valueOf();

        if (now.getMonth() == 0) {
            current = new Date(now.getFullYear() - 1, 11, 1);
        } else {
            current = new Date(now.getFullYear(), now.getMonth() - 1, 1);
        }

    })(jQuery);

    $(document).ready(function(){
        var myPicker = new MyDatePicker('#picker','#widgetFieldDateRange',siteData.datepicker.format,(new Date()).addMonths(-1),new Date(),siteData.datepicker.translations);
        myPicker.setPickerType(2);
        myPicker.setTrigger('#flatDatePickerCanvasHol');

        myPicker.setFirstDay(siteData.datepicker.firstDay);
        myPicker.setDayNames(siteData.datepicker.dayNames);

        myPicker.setRtl(siteData.htmlDirection);
        myPicker.setMonthNamesShort(siteData.datepicker.monthNamesShort);
        myPicker.setMonthNames(siteData.datepicker.monthNames);

        myPicker.setApplyButtonText(siteData.datepicker.applyButton);

        myPicker.attachTo((siteData.htmlDirection == 'rtl') ? 'opposite' : 'normal', 0, 0, 1, '#flatDatePickerCanvasHol');

        myPicker.setMaxDate(0);
        myPicker.setMinDate(new Date(0));
        myPicker.create();

        var firstTimePicker=true;

        $(myPicker.input).on('onClose', function (e,selectedText, currentInstance, dates, applyBtn) {
                if(applyBtn){
                    historical_submit();
                    firstTimePicker=false;
                }

        });

        function historical_submit(isInterval) {
            var isDatePicked = !isInterval;
            if(!firstTimePicker) isDatePicked=true;
            var dateFormat = $.datepicker.regional[''].dateFormat;
            var dateTo =new Date();
            var dateFrom = new Date();
            var intervalInDays=30;
            if(isDatePicked) {
                var dates = $.datepicker._getFromToDates($.datepicker._curInst,'yy-mm-dd');
                if (dates !== null) {
                    dateFrom=dates.dateFrom;
                    dateTo=dates.dateTo;
                }
            }
            else {
                if ($('#data_interval').val() === 'Weekly') {intervalInDays = 52 * 7}; //365 - full year
                if ($('#data_interval').val() === 'Monthly') {intervalInDays = 30 * 24}; //~ 2 years
                var dateFromObj=new Date();
                dateFromObj.setDate(dateFromObj.getDate() - intervalInDays);
                dateFrom= dateFromObj;//$.datepicker.formatDate(dateFormat,dateFromObj);
            }

            var datesStrings = {
                fromInputStr : $.datepicker.formatDate(dateFormat,dateFrom),
                toInputStr : $.datepicker.formatDate(dateFormat,dateTo)
            };

            $('#picker').val( datesStrings.fromInputStr +' - ' + datesStrings.toInputStr);
            $('#widgetFieldDateRange').text($('#picker').val());
            var _ajaxLoadingVar = '<div class="loading-responsive medium-circle tableLoading"></div>';
            $("#results_box").html( _ajaxLoadingVar );

            $.post('/instruments/HistoricalDataAjax',
                {
                    action: 'historical_data',
                    curr_id: 5,
                    st_date: datesStrings.fromInputStr,
                    end_date: datesStrings.toInputStr,
                    interval_sec: $('#data_interval').val()},
                function(data) {
                    parse_ajax_response( data );
                }, "text" );
        }

        function parse_ajax_response( _data ) {
            _data=_data.replace('<div id="results_box">','').replace(/<\/div>/,'');
            $("#results_box").html( _data );
        }

        $('#data_interval').change(function() {
            historical_submit(true);
        });
    });


</script>
		
		
		
		
		
	<div class="doubleLineSeperator"></div>
<div class="instrumentsSentiments" id="sentiments_5">
    <h2><a href="/members-admin/sentiments/currency">My Sentiments</a></h2>
    <div class="left" id="sentiments_box_container_5">
                    <script>
function sendVote(val) {


    var data = {
        action          : 'add_comment',
        type            : val,
        conviction      : "firm",
        timeframe       : 2592000,
        pair_ID         : $('#item_id_5').val(),
        pair_category   : $('#sentiments_category_5').val()

    };

    if(!window.siteData.userLoggedIn) {
        overlay.authCompleteAction = {
            type: 'sentiment',
            actionData: data
        };
        overlay.overlayLogin();
        return;
    }

	$.get("/common/sentiments/sentiments_ajax.php", data, function(data) {

        // If user is not logged in - redirect to Login page (all data we've sent is saved in session cookie)
        if( data === 'auth_failed' ) {
            // window.location = window.login_url;
            $('#bearButton_5').removeClass('selected').addClass('genToolTip');
            $('#bullButton_5').removeClass('selected').addClass('genToolTip');
            overlay.overlayLogin();

        // If data sanitization failed
        } else if( data === 'wrong_params' ) {
            return false;

        // If we didn't receive Auth nor Wrong_params error, we received JSON with HTML blocks..
        } else {

            try {
	$('#tpl_loading_spin_5').addClass('inlineblock');
 	$('#tpl_loading_spin_5').removeClass('displayNone');
 	$('#sentiment_submit_5').addClass('displayNone');
		$('#sentiment_submit_5').removeClass('inlineblock');
                var json = JSON.parse(data);
                // Update Stats block with what we received from server
                $('#sentiments_stats_5').html(json.stats);
                // Display Thank you message from template
                $('#sentiments_box_container_5').html( json.thank_you );
            } catch(e) {}

        }

    });	
}

function sentiment_change_type(element, sentiment_type) {

    if (!$(element).hasClass('selected')) {
        var gaLabel;
        $(element).addClass('selected').removeClass('genToolTip');
        if (element.id == 'bullButton_5') {
            $('#bearButton_5').removeClass('selected').addClass('genToolTip');
            sendVote('bullish');
            gaLabel = 'Bull';
        } else {
            $('#bullButton_5').removeClass('selected').addClass('genToolTip');
            gaLabel = 'Bear';
            sendVote('bearish');
        }

        if(siteData.userLoggedIn) {
            gaLabel += ' - Logged In';
        } else {
            gaLabel += ' - Not Logged In';
        }

        ga('allSitesTracker.send', 'event', 'Social Buttons', 'My Sentiment', gaLabel);
    }
}

</script>

<div class="votingBox inlineblock">
    <span class="bold middle">Add your sentiment:</span>
    <div class="voteButtons inlineblock middle">
        <label class="newBtn LightGray newSentimentsButton inlineblock genToolTip oneliner boldTip" data-tooltip="Bullish" for="bullRadio" id="bullButton_5" name="bull" onclick="sentiment_change_type(this,'bullish')"><span class="newBullIcon2"></span></label>
        <div class="inlineblock align_center or">or</div>
        <label class="newBtn LightGray newSentimentsButton inlineblock genToolTip oneliner boldTip" data-tooltip="Bearish" for="bearRadio" id="bearButton_5" name="bear" onclick="sentiment_change_type(this,'bearish')"><span class="newBearIcon2"></span></label>
    </div>
</div>
